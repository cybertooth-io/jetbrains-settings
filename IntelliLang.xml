<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="Handlebars" injector-id="js">
      <display-name>Handlebars in JS strings</display-name>
      <prefix>hbs`</prefix>
      <place><![CDATA[isHtmlLikeText()]]></place>
    </injection>
    <injection language="JSX Harmony" injector-id="js">
      <display-name>React JSX in JS strings</display-name>
      <place disabled="true"><![CDATA[isReactJSXLikeText()]]></place>
    </injection>
  </component>
</application>